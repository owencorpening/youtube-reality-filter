{
  "manifest_version": 3,
  "name": "YouTube Reality Filter",
  "version": "1.0",
  "description": "An AI-powered skeptic button to analyze YouTube titles for logical holes.",
  "permissions": ["activeTab"],
  "host_permissions": ["https://script.google.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_title": "YouTube Reality Filter"
  }
}